script/source = "extends RigidBody

var pick = false
var gravPick = false
var interact = false
var pickUP = false

func _process(delta):
	if gravPick == true:
		gravity_scale = 0
		collision_layer = 2
		yield(get_tree().create_timer(.1),\"timeout\")
		interact = true
	elif gravPick == false:
		gravity_scale = 1
		collision_layer = 1
		interact = false
	if GlobalWorld.player.carrying == false:
		gravPick = false

